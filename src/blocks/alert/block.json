{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "rrze-elements/alert",
  "version": "1.0.16",
  "title": "Alert",
  "category": "rrze_elements",
  "description": "Used to highlight important information and warnings.",
  "attributes": {
    "style": {
      "type": "string",
      "default": "default"
    },
    "color": {
      "type": "string",
      "default": "#e9edf2"
    },
    "border_color": {
      "type": "string",
      "default": ""
    },
    "title": {
      "type": "string",
      "default": ""
    },
    "textColor": {
      "type": "string",
      "default": "#000000"
    },
    "borderColor": {
      "type": "string",
      "default": ""
    }
  },
  "example": {
    "attributes": {
      "style": "success",
      "color": "#e9edf2",
      "border_color": "",
      "title": "Alert",
      "textColor": "#000000",
      "borderColor": ""
    },
    "innerBlocks": [
      {
        "name": "core/paragraph",
        "attributes": {
          "content": "This is an alert box."
        }
      }
    ]
  },
  "supports": {
    "html": false
  },
  "textdomain": "rrze-elements-blocks",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}