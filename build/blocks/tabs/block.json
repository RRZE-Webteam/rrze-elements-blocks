        },
        {
          "clientId": "eabf0d78-b231-4cb3-8d3e-02840f1a8e68",
          "title": "Vierter Reiter",
          "position": 2,
          "icon": "solid angles-up",
          "svgString": "fa-solid fa-angles-up"
        },
        {
